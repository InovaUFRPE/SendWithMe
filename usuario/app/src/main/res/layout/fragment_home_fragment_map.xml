<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomePackage.HomeFragmentMap">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/cidadeDigitadaId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="7dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Carpina"
            app:layout_constraintBottom_toTopOf="@+id/cidadeDigitadaId2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <EditText
            android:id="@+id/cidadeDigitadaId2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="7dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="TracunhaÃ©m"
            app:layout_constraintBottom_toTopOf="@+id/botaoAchaCidadeId"
            app:layout_constraintStart_toStartOf="@+id/cidadeDigitadaId"
            app:layout_constraintTop_toBottomOf="@+id/cidadeDigitadaId" />

        <Button
            android:id="@+id/botaoAchaCidadeId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FIND ROUTE"
            app:layout_constraintBottom_toTopOf="@+id/container2Id"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cidadeDigitadaId2" />

        <FrameLayout
            android:id="@+id/container2Id"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/botaoAchaCidadeId">

            <org.osmdroid.views.MapView android:id="@+id/mapaId"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </FrameLayout>

    </android.support.constraint.ConstraintLayout>

</FrameLayout>